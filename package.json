  "version": "1.0.0",
  "description": "Bot de automatización de trámites gubernamentales de Argentina",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "tsc",
    "dev": "ts-node src/main.ts",
